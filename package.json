{
  "name": "loginx",
  "version": "1.0.0",
  "description": "A modern, secure authentication and user management mobile application built with Expo and React Native",
  "main": "expo-router/entry",
  "author": {
    "name": "Vivek Barsagadey",
    "email": "vivek@whizit.co.in"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vivekbarsagadey/loginx.git"
  },
  "bugs": {
    "url": "https://github.com/vivekbarsagadey/loginx/issues"
  },
  "homepage": "https://github.com/vivekbarsagadey/loginx#readme",
  "keywords": [
    "expo",
    "react-native",
    "authentication",
    "login",
    "mobile-app",
    "firebase",
    "typescript",
    "expo-router",
    "user-management",
    "2fa",
    "biometric-authentication",
    "secure-storage",
    "i18n",
    "dark-mode",
    "accessibility",
    "open-source",
    "Whiz IT"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "start": "expo start --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android --tunnel --clear",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "lint:md": "markdownlint \"**/*.md\" --ignore node_modules --ignore .expo",
    "lint:md:fix": "markdownlint \"**/*.md\" --ignore node_modules --ignore .expo --fix",
    "lint:md:remark": "remark . --quiet --frail",
    "lint:md:remark:fix": "remark . --output",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:md": "prettier --write \"**/*.md\" && remark . --output",
    "format:md:check": "prettier --check \"**/*.md\"",
    "format:md:prettier": "prettier --write \"**/*.md\"",
    "format:md:remark": "remark . --output",
    "type-check": "tsc --noEmit",
    "validate": "pnpm lint && pnpm lint:md && pnpm lint:md:remark && pnpm format:check && pnpm type-check",
    "validate:fix": "pnpm lint:fix && pnpm lint:md:fix && pnpm lint:md:remark:fix && pnpm format"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^15.0.2",
    "@hookform/resolvers": "^5.2.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@sentry/react-native": "~7.2.0",
    "@types/expo__vector-icons": "^9.0.1",
    "dotenv": "^17.2.3",
    "expo": "54.0.12",
    "expo-apple-authentication": "^8.0.7",
    "expo-constants": "~18.0.9",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.8",
    "expo-local-authentication": "~17.0.7",
    "expo-localization": "~17.0.7",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.10",
    "expo-secure-store": "^15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "firebase": "^12.3.0",
    "i18n-js": "^4.5.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-native": "0.81.4",
    "react-native-biometrics": "^3.0.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-google-places-autocomplete": "^2.5.7",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zod": "^3.25.76",
    "zod-form-data": "^3.0.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "markdownlint-cli": "^0.43.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "remark-cli": "^12.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-lint": "^10.0.0",
    "remark-lint-emphasis-marker": "^4.0.0",
    "remark-lint-list-item-indent": "^4.0.0",
    "remark-lint-maximum-line-length": "^4.1.1",
    "remark-lint-strong-marker": "^4.0.0",
    "remark-preset-lint-consistent": "^6.0.0",
    "remark-preset-lint-recommended": "^7.0.0",
    "typescript": "~5.9.2"
  },
  "private": true,
  "pnpm": {
    "overrides": {
      "@types/react": "~19.1.0",
      "@types/react-native": "npm:react-native@0.81.4",
      "@react-native-async-storage/async-storage": "^2.2.0"
    }
  }
}
