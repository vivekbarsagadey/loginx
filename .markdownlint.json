{
  "default": true,
  "MD013": {
    "line_length": 400,
    "heading_line_length": 400,
    "code_block_line_length": 400,
    "code_blocks": true,
    "tables": false,
    "headings": true,
    "strict": false,
    "stern": false
  },
  "MD003": {
    "style": "atx"
  },
  "MD004": {
    "style": "dash"
  },
  "MD007": {
    "indent": 2
  },
  "MD024": {
    "siblings_only": true
  },
  "MD025": {
    "level": 1
  },
  "MD026": {
    "punctuation": ".,;:!?"
  },
  "MD029": {
    "style": "one_or_ordered"
  },
  "MD030": {
    "ul_single": 1,
    "ol_single": 1,
    "ul_multi": 1,
    "ol_multi": 1
  },
  "MD033": {
    "allowed_elements": ["details", "summary", "br", "sub", "sup"]
  },
  "MD034": false,
  "MD040": false,
  "MD041": false,
  "MD046": {
    "style": "fenced"
  },
  "MD048": {
    "style": "backtick"
  },
  "MD051": false
}
